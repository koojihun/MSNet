<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.msnet.view.ProductInfoDialogController">
   <children>
      <VBox prefHeight="500.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="productInfoTableView" prefHeight="466.0" prefWidth="500.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="258.0" text="Name" />
                <TableColumn fx:id="idColumn" prefWidth="240.0" text="ID" />
                  <TableColumn fx:id="productionDateColumn" prefWidth="75.0" text="Production date" />
                  <TableColumn fx:id="expirationDateColumn" prefWidth="75.0" text="Expiration date" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ButtonBar>
              <buttons>
                <Button minWidth="71.0" mnemonicParsing="false" prefHeight="23.0" text="Send" />
                  <Button mnemonicParsing="false" text="OK" />
              </buttons>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
